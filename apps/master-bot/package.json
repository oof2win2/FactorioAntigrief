	"dependencies": {
		"@discordjs/builders": "^0.10.0",
		"@discordjs/rest": "^0.1.0-canary.0",
		"@prisma/client": "^3.7.0",
		"@types/node": "^16.6.1",
		"discord-api-types": "^0.22.0",
		"discord.js": "^13.4.0",
		"dotenv": "^10.0.0",
		"envalid": "^7.2.1",
		"fagc-api-wrapper": "^1.28.2",
		"install-types": "^2.0.1",
		"nodemon": "^2.0.12",
